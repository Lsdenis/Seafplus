<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<title>@ViewBag.Title</title>
	<link href="../../Content/css/bootstrap.css" rel="stylesheet" />
	<link href="../../Content/css/custom.css" rel="stylesheet" />
</head>

@Html.Partial("_JsUrls")

<body>
	<div>
		<div class="container">
			<div id="header">
				@Html.Partial("_Header")
			</div>
			<div>
				@RenderBody()
			</div>
		</div>
		<div id="footer">
			<div class="container">
				@Html.Partial("_Footer")
			</div>
		</div>
	</div>
</body>
</html>
